  "name": "cccatalog-frontend",
  "version": "1.0.0",
  "description": "Common Creative search",
  "author": "sebworks",
  "private": true,
  "type": "module",
  "scripts": {
    "server": "node server.js",
    "dev": "webpack-dev-server --inline --progress --config build/webpack-dev-conf.js",
    "dev-secure": "webpack-dev-server --https --inline --progress --config build/webpack-dev-conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest-conf.js --coverage",
    "qa-install": "brew install geckodriver && brew install pipenv && cd test/selenium-webdriver && pipenv install",
    "qa": "cd test/selenium-webdriver && pipenv run python search_screencaps.py",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue . test/unit",
    "build": "node build/build.js && npm run build-server",
    "build-server": "webpack -p --config build/webpack-server-conf.js --progress"
  },
